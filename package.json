{
  "name": "typescript-typedefs",
  "version": "0.3.5",
  "license": "MIT",
  "author": "Bram Kaashoek",
  "repository": {
    "type": "git",
    "url": "https://github.com/BramKaashoek/typescript-typedefs"
  },
  "keywords": [
    "GraphQL",
    "TypeScript",
    "typeDefs",
    "interfaces",
    "decorators"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint --ext .ts src/",
    "tst": "mocha --require ts-node/register --recursive 'src/**/*.spec.ts' --timeout 8000",
    "test": "npm run tst  && npm run lint",
    "test:watch": "nodemon -e ts -w ./src --exec npm run tst",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src",
    "postversion": "git push && git push --tags",
    "publish": "npm publish dist"
  },
  "dependencies": {
    "graphql": "^14.3.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "chai": "^4.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "prettier": "^1.17.1",
    "ts-node": "^8.2.0",
    "typescript": "^3.4.5"
  }
}
