    "test:watch": "nodemon -e ts -w ./src --exec npm run tst"
  },
  "dependencies": {
    "graphql": "^14.3.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.6",
    "@typescript-eslint/eslint-plugin": "^1.9.0",
    "@typescript-eslint/parser": "1.9.0",
    "chai": "^4.2.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.0",
    "ts-node": "^8.0.2",
    "typescript": "^3.4.5"
  }
}
